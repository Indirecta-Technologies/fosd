local module = {
	-- iComm Circle App
	["AppInfo"] =  {
		["Icon"] = "10607987614";
		["Name"] = "Sprint Pager";
		["SoftKeys"] = {"","",""};
		["Revision"] = "1.3";
	};

}

function module:Init(name, value)
	if not self[name] and value then self[name] = value else return false end
	return true
end

module["pages"] = {}

function module:UpdatePageCount()
	self["AppFrame"]["FrontPage"].DoubleArrow.Visible = #self["pages"] > 0
	self["AppFrame"]["FrontPage"].PagesText.Text = #self["pages"] == 1 and "View 1 Page" or (#self["pages"] == 0 and "No new pages" or "View "..#self["pages"].." pages")
	for i,v in ipairs(module["pages"]) do
		v.StickyNote.PagesLeft.Text = i.."/"..#module["pages"]
	end
end

function module:AddPage(title, text)
	local newPage = script.PageTemplate:Clone();
	newPage.StickyNote.PageText.Text = text
	newPage.StickyNote.PageTitle.Text = title
	table.insert(module["pages"], newPage)
	self:UpdatePageCount();
	newPage.Parent = self["AppFrame"]
	newPage.Name = "SprintPage"..#module["pages"]
	script.Alert:Play()

	if not self["AppFrame"].Visible then
		self.deps["screen"].notification(title,text, "rbxassetid://"..self["AppInfo"]["Icon"], Color3.fromRGB(231, 39, 57))
		if self["Button"] then
			if self["Button"].Notifs.Visible == true then
				self["Button"].Notifs.Text = tonumber(self["Button"].Notifs.Text) and ((tonumber(self["Button"].Notifs.Text) or 0) + 1) or "1"
			else
				self["Button"].Notifs.Visible = true
				self["Button"].Notifs.Text = "1"
			end
		end
	end

end

function module:Open(Button)

	self:Init("AppFrame", script:FindFirstChild("AppFrame"))
	self:Init("Button", Button)
	self["AppFrame"].Name = self["AppInfo"]["Name"]
	self["AppFrame"].Parent = self.deps["screen"].components["mainscreen"]
	self.deps["screen"].components.statusbar.Macro.Text = self["AppInfo"]["Name"]
	self.deps["screen"].showOnly(self["AppInfo"]["Name"])
	self.deps["screen"].bottom_bar(table.unpack(self["AppInfo"]["SoftKeys"]))

	if Button.Notifs.Visible == true then
		Button.Notifs.Visible = false
		Button.Notifs.Text = "0"
	end

	script.Sprint.OnClientEvent:Connect(function(g, t, s)
		self:AddPage(t,s)
	end)

end

module.latch = false

function module:Activate(deps, k, cc)
	-- Scrolling Frame UP/DOWN Smooth Navigation
	if (k == "UP") then
		if cc:IsA("ScrollingFrame") and cc:GetAttribute("PageIndex") then
			self.deps["screen"].scrollingPageUp(cc)
			return
		end
	end
	if (k == "DOWN") then
		if cc:IsA("ScrollingFrame") and cc:GetAttribute("PageIndex") then
			self.deps["screen"].scrollingPageDown(cc)
			return
		end
	end
	if (k == "OK") then
		local currentPage = self["pages"][cc:GetAttribute("PageIndex")-1]
		if currentPage and not module.latch then

			local Info = TweenInfo.new(0.4, Enum.EasingStyle.Quad, Enum.EasingDirection.Out)
			local Tween = game:GetService("TweenService"):Create(currentPage.StickyNote, Info,
			{Position = UDim2.new(1,0,0,0)});

			Tween:Play()
			module.latch = true
			Tween.Completed:Wait()

			currentPage:Destroy()
			table.remove(self["pages"], cc:GetAttribute("PageIndex")-1)
			self:UpdatePageCount();
			script.Alert:Stop()
			module.latch = false

			self.deps["screen"].scrollingPageUp(cc)

		end

	end
	if (k == "LFN") then
		self:AddPage("Test","Lorem Ipsum Dolor Sit Amet")
	end
	if (k == "Back") then
		self.deps["screen"].showOnly("Menu")
		self.deps["screen"].bottom_bar()
	end
end


return module
